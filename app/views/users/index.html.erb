<div class="container">
  <div class='row'>
    <div class='col-10 align-self-center'>
      <lu class="list-group">
        <% @users.each do |user| %>
        <li class='list-group-item user-id-<%= user.id %>'>
          <%= link_to user.nickname, user_path(user), method: :get %>
          <% if user.online_status %>
            <span class="glyphicon glyphicon-ok">online</span>
          <% end %>
        </li>
        <% end %>
    </div>
  </div>
</div>