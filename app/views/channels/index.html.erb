<div class='container'>
 <div class="row">
  <div class='col-10 align-self-center">'>
    <% if @channels.present? %>
      <div class="list-group">
        <span class="list-group-item">Чаты</span>
        <% @channels.each do |channel| %>
          <%= link_to channel.title, channel_path(channel), method: :get, class: 'list-group-item ' %>
        <% end %>
      </div>
    <% end %>

    <%= link_to new_channel_path do %>
      Создать новый канал
    <% end %>
    <%= link_to sessions_path, method: :delete do %>
      Создать новый канал
    <% end %>
  </div>
</div>
</div>
</div>